{% extends 'base.html.twig' %}

{% block body %}
    <h1>Panstwo</h1>
    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ panstwa.id }}</td>
            </tr>
            <tr>
                <th>Nazwa</th>
                <td>{{ panstwa.nazwa }}</td>
            </tr>
            <tr>
                <th>Skrot</th>
                <td>{{ panstwa.skrot }}</td>
